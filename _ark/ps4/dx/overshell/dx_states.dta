            {$this trigger_select_effects}
         )
         (('HUD: ON' 'HUD: OFF')
            {set $noui {! $noui}}
            {if_else $noui
               {write_file 'data:/GoldHen/RB4DX-1.08/ps4/ui/game/hud.scene_ps4' ()}
