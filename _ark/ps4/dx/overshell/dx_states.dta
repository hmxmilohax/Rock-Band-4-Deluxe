            }
            {$this trigger_select_effects}
         )
         (('Hide Mic Warning: ON' 'Hide Mic Warning: OFF')
            {set $micwarn {! $micwarn}}
            {if_else $micwarn
               {do
                  {dx_file_rename 'ps4/ui/game/hud1.scene_ps4' 'ps4/ui/game/hud.scene_ps4'}
